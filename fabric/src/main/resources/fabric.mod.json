{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "Greenhouse Team",
        "MerchantPug"
    ],
    "contact": {
        "homepage": "https://github.com/GreenhouseTeam/reloadable-datapack-registries",
        "sources": "https://github.com/GreenhouseTeam/reloadable-datapack-registries",
        "issues": "https://github.com/GreenhouseTeam/reloadable-datapack-registries/issues"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
          "dev.greenhouseteam.rdpr.impl.client.RDPRClientFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric-api": "*",
        "minecraft": ">=1.20.4",
        "java": ">=17"
    }
}
  